    "def unique_labels(labels):\n",
    "    unique = []\n",
    "    for y in labels:\n",
    "        for technique in y:\n",
    "            if technique.strip() not in unique:\n",
    "                unique.append(technique.strip())\n",
    "    return unique"
   ]
